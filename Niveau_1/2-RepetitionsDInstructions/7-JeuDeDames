class Main{
   public static void main(String[] args){
   
      for (int i = 0; i < 20; i++){
         for (int j = 0; j < 10; j++){
            System.out.print("OXOX");
         }
         System.out.println();
         
         for (int k = 0; k < 10; k++){
            System.out.print("XOXO");  
         }
         System.out.println();
      }
      
   }
}
